  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-memcache": "*",
    "ext-memcached": "*",
    "ext-mongodb": "*",
    "ext-openssl": "*",
    "bunkerdb/date_range": "1.0.5",
    "cratia/adapter-mysql": "^0.2.5",
    "cratia/core-api-rest": ">=0.5",
    "cratia/interface-adapter": "^0.2.2",
    "cratia/orm-dql": "^0.4.7",
    "cratia/pipeline": ">=0.7",
    "dabreu/api-client-skeleton": "^2",
    "doctrine/dbal": "^2.10",
    "doesntmattr/mongodb-migrations": "^3.0",
    "friendsofphp/php-cs-fixer": "^3.3",
    "league/flysystem": "^2.0",
    "mongodb/mongodb": "1.8.0",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.0",
    "phpmd/phpmd": "^2.10",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4",
    "symfony/dotenv": "^5.0",
    "symfony/var-dumper": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
      "files": [
        "app/config.php"
      ],
      "classmap": [
        "src/"
      ],
      "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.1"
  }
}
